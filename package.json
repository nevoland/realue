{
  "name": "realue",
  "private": false,
  "version": "4.0.0-alpha.35",
  "type": "module",
  "main": "./dist/main.js",
  "exports": {
    "types": "./dist/main.d.ts",
    "import": "./dist/main.js"
  },
  "files": [
    "dist",
    "lib",
    "!lib/old",
    "!lib/**/*.test.ts?",
    "!src",
    "!src/old",
    "README.md",
    "doc/",
    "!doc/.nojekyll"
  ],
  "scripts": {
    "dev": "vite",
    "dev:test": "vitest",
    "build": "rm -rf dist/; tsc --project tsconfig.package.json;",
    "build:doc": "typedoc",
    "build:doc:save": "typedoc --tsconfig tsconfig.package.json && git add -A && git commit -a -m 'Update doc'",
    "prepare": "npm run build",
    "start": "vite preview",
    "format": "prettier --check \"{src,scripts}/**/*.[jt]s\"",
    "format:fix": "prettier --write \"{src,scripts}/**/*.[jt]s\"",
    "test": "vitest run && tsc && npm run format",
    "lint": "npm run lint:ts",
    "lint:fix": "npm run lint:ts:fix",
    "lint:ts": "eslint .",
    "lint:ts:fix": "eslint --fix .",
    "release:patch": "npm version patch && git push origin --follow-tags && npm publish",
    "release:minor": "npm version minor && git push origin --follow-tags && npm publish",
    "release:major": "npm version major && git push origin --follow-tags && npm publish",
    "release:alpha": "npm version prerelease --preid=alpha && git push origin --follow-tags && npm publish --tag=next",
    "release:beta": "npm version prerelease --preid=beta && git push origin --follow-tags && npm publish --tag=next"
  },
  "dependencies": {
    "@preact/signals": "^1.2.1",
    "@types/lodash-es": "^4.17.9",
    "is-promise": "^4.0.0",
    "lodash-es": "^4.17.21",
    "preact": "^10.19.2",
    "unchangeable": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.53.0",
    "@preact/preset-vite": "^2.5.0",
    "@types/node": "^20.8.0",
    "@typescript-eslint/parser": "^6.7.5",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.51.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-inferno": "^7.32.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-vitest": "^0.3.2",
    "futurise": "^1.2.3",
    "nanoid": "^5.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-module-list": "^2.1.0",
    "vitest": "^0.34.6"
  }
}
